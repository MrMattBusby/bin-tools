__version__ = "1.0.0 (See git history)"

import sys

if __name__ == '__main__':
  outs = ''
  for each in zip(*(line.strip().split() for line in sys.stdin.readlines() if line.strip())):
    outs += ' '.join(each) + '\n'
  sys.stdout.write(outs)
